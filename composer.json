{
    "name": "babdev/michaels-website",
    "description": "Source code for https://michaels.website",
    "license": "GPL-2.0+",
    "type": "project",
    "authors": [
        {
            "name": "Michael Babker",
            "email": "michael.babker@gmail.com",
            "homepage": "https://www.babdev.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^7.4",
        "fig/link-util": "^1.1.1",
        "joomla/application": "^2.0@dev",
        "joomla/controller": "^2.0@dev",
        "joomla/di": "^2.0@dev",
        "joomla/event": "^2.0@dev",
        "joomla/input": "^2.0@dev",
        "joomla/preload": "^2.0@dev",
        "joomla/registry": "^2.0@dev",
        "joomla/renderer": "^2.0@dev",
        "joomla/router": "^2.0@dev",
        "joomla/uri": "^2.0@dev",
        "laminas/laminas-diactoros": "^2.3",
        "pagerfanta/pagerfanta": "^2.1.3",
        "psr/link": "^1.0",
        "spatie/yaml-front-matter": "^2.0.5",
        "symfony/asset": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/property-access": "^5.1",
        "symfony/property-info": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/web-link": "^5.1",
        "symfony/yaml": "^5.1",
        "twig/twig": "^2.12.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16.3",
        "maximebf/debugbar": "^1.16.1"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\Website\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4.6"
        }
    }
}
