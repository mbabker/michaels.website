{
    "name": "babdev/michaels-website",
    "description": "The michaels.website website",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "composer-runtime-api": "^2.0",
        "babdev/laravel-server-push-manager": "2.0.0",
        "elhebert/laravel-sri": "3.0.1",
        "laravel/framework": "8.83.0",
        "spatie/laravel-sitemap": "6.1.0",
        "spatie/schema-org": "3.8.0",
        "spatie/sheets": "1.8.1",
        "spatie/yaml-front-matter": "2.0.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.12.1",
        "facade/ignition": "2.17.4",
        "mockery/mockery": "1.5.0",
        "nunomaduro/larastan": "1.0.3",
        "phpstan/extension-installer": "1.1.0",
        "phpstan/phpstan": "1.4.6",
        "phpstan/phpstan-mockery": "1.0.0",
        "phpstan/phpstan-phpunit": "1.0.0",
        "phpunit/phpunit": "9.5.13"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1.2"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
