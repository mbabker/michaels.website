{
    "name": "babdev/michaels-website",
    "description": "Source code for https://michaels.website",
    "license": "GPL-2.0+",
    "type": "project",
    "authors": [
        {
            "name": "Michael Babker",
            "email": "michael.babker@gmail.com",
            "homepage": "https://www.babdev.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": ">=7.2",
        "fig/link-util": "~1.0",
        "joomla/application": "~2.0@dev",
        "joomla/controller": "~2.0@dev",
        "joomla/di": "~2.0@dev",
        "joomla/event": "~2.0@dev",
        "joomla/input": "~2.0@dev",
        "joomla/preload": "~2.0@dev",
        "joomla/registry": "~2.0@dev",
        "joomla/renderer": "~2.0@dev",
        "joomla/router": "~2.0@dev",
        "joomla/uri": "~2.0@dev",
        "pagerfanta/pagerfanta": "~2.0",
        "phpdocumentor/reflection-docblock": "~4.3",
        "psr/link": "~1.0",
        "spatie/yaml-front-matter": "^2.0",
        "symfony/asset": "4.2.*",
        "symfony/finder": "4.2.*",
        "symfony/property-access": "4.2.*",
        "symfony/property-info": "4.2.*",
        "symfony/serializer": "4.2.*",
        "symfony/web-link": "4.2.*",
        "symfony/yaml": "4.2.*",
        "twig/extensions": "^1.5.1",
        "twig/twig": "^2.4.8",
        "zendframework/zend-diactoros": "~2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.12",
        "maximebf/debugbar": "^1.13"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\Website\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
